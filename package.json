{
  "name": "hide-my-files",
  "publisher": "ahvmckay",
  "displayName": "Hide My Files",
  "description": "Hide files from the explorer view",
  "repository": "https://github.com/alexandermckay/hide-my-files__extension.git",
  "version": "0.3.0",
  "engines": {
    "vscode": "^1.37.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "hide",
    "files",
    "includes",
    "files.exclude"
  ],
  "activationEvents": [
    "onCommand:extension.hideFiles"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.hideFiles",
        "title": "Hide Files"
      }
    ]
  },
  "scripts": {
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.37.0",
    "eslint": "^5.13.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "typescript": "^3.3.1",
    "vscode-test": "^1.0.2"
  },
  "dependencies": {
    "rambda": "4.0.1"
  }
}
